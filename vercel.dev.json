{
  "images": {
    "domains": [
      "a.storyblok.com"
    ],
    "minimumCacheTTL": 60,
    "formats": [
      "image/avif",
      "image/webp"
    ],
    "dangerouslyAllowSVG": true,
    "sizes": [
      160,
      320,
      640,
      960,
      1280,
      1600,
      1920
    ],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "a.storyblok.com"
      }
    ]
  },
  "routes": [
    {
      "src": "/.*",
      "middlewarePath": "middleware"
    }
  ]
}